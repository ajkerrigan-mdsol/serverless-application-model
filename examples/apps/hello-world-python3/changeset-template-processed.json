{
    "AWSTemplateFormatVersion": "2010-09-09", 
    "Parameters": {
        "Environment": {
            "Default": "sandbox", 
            "Type": "String", 
            "Description": "Analogous to a Medistrano stage/environment name"
        }, 
        "Project": {
            "Type": "String", 
            "Description": "Analogous to a Medistrano project name"
        }, 
        "BasicExecutionRoleName": {
            "Default": "lambda-basic-execution", 
            "Type": "String", 
            "Description": "Name of an existing execution role for this function"
        }
    }, 
    "Description": "A starter AWS Lambda function.", 
    "Resources": {
        "helloworldpython3": {
            "Type": "AWS::Lambda::Function", 
            "Properties": {
                "Code": {
                    "S3Bucket": "sam-packages-2c6f24a1", 
                    "S3Key": "helloworld/2e86b15a1bea1eb7071edede856bc5bc"
                }, 
                "FunctionName": {
                    "Fn::Sub": "${Project}-${Environment}-HelloFunction"
                }, 
                "Tags": [
                    {
                        "Value": "SAM", 
                        "Key": "lambda:createdBy"
                    }
                ], 
                "MemorySize": 128, 
                "Handler": "lambda_function.lambda_handler", 
                "Role": {
                    "Fn::Sub": "arn:aws:iam::${AWS::AccountId}:role/${BasicExecutionRoleName}"
                }, 
                "Timeout": 3, 
                "Runtime": "python3.6", 
                "Description": "A starter AWS Lambda function."
            }
        }
    }
}
